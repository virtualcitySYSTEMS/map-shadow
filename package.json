{
  "name": "@vcmap/shadow",
  "version": "3.0.0-rc.0",
  "mapVersion": "^6.0.0",
  "description": "shadow tool to change daylight settings",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "pack": "vcmplugin pack",
    "start": "vcmplugin serve",
    "preview": "vcmplugin preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix"
  },
  "author": "virtualcitysystems GmbH",
  "license": "MIT",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "plugin-assets/",
    "LICENSE.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "./src/index.js",
    "./dist": "./dist/index.js"
  },
  "repository": {
    "url": "git@gitlab.virtualcitysystems.de:vcsuite/npm/vcmap/shadow.git"
  },
  "peerDependencies": {
    "@vcmap-cesium/engine": "^11.0.2",
    "@vcmap/core": "^6.0.0-rc.7",
    "@vcmap/ui": "^6.0.0-rc.7",
    "vue": "~3.4.27",
    "vuetify": "^3.6.13"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "^4.0.0-rc.4",
    "@vcsuite/eslint-config": "^3.0.8",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.38.0",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^24.0.0",
    "resize-observer-polyfill": "1.5.1",
    "vitest": "^1.6.0"
  },
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "eslintIgnore": [
    "node_modules",
    "*timestamp-*.mjs",
    "dist"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@vcsuite/eslint-config/vue"
  }
}
